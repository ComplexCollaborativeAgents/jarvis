sp {jarvis*propose*maintain-world-state*1
	(state <s>	^name jarvis
            	^world <w>
             	^io.input-link.world <il>)
 	(<il>	^component <c>)
  	(<c>	^name <name>
        	^<attr> {<val> <> <name>})
   	(<w>	^component <wc>)
    (<wc>	^name <name>)
-->
	(<s>	^operator <op> + =)
 	(<op>	^name maintain-world-state
         	^input-link-id <c>
          	^world-id <wc>
           	^type object-sensed)
  }

sp {jarvis*propose*maintain-world-state*2
	(state <s>	^name jarvis
            	^world <w>
             	^io.input-link.world <il>)
 	(<w>	^component <c>)
  	(<c>	^visible true
        	^name <name>)
   	-{(<il>	^component <any>)
      (<any>	^name <name>)}
-->
	(<s>	^operator <op> + =)
 	(<op>	^name maintain-world-state
        	^world-id <c>
         	^type object-not-sensed)
  }