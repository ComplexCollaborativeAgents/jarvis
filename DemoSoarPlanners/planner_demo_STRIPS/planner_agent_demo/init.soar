sp {propose*init-agent
    (state <s>    ^superstate nil
                  -^name)
-->
    (<s>    ^operator <op> +)
    (<op>    ^name init-agent)
    }

sp {apply*init-agent
    (state <s>    ^operator.name init-agent)
-->
    (<s>    ^name coach-ara
            ^previous-operator nil)
    }
